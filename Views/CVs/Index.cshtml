@model IEnumerable<data.Models.CV>

@{
    ViewBag.Title = "Index";
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>



    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>

            <td>
                <a href="~/CVs/Details/@item.id" class="btn btn-default ">View profile</a>
            </td>

        </tr>
    }

</table>
